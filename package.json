    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-eventemitter": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "jsdoc-strip-async-await": "^0.1.0",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "sinon": "^6.1.4",
    "uglify-js": "^3.4.6",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "preexample": "npm run build",
    "example": "DEBUG=flowai:* node ./example/node",
    "prebuild": "npm run docs",
    "build:dist": "BABEL_ENV=dist NODE_ENV=production webpack --display-error-details --config ./webpack.config.js",
    "build:lib": "BABEL_ENV=lib NODE_ENV=production babel ./src --out-dir ./lib",
    "build": "npm run build:dist && npm run build:lib",
    "test": "BABEL_ENV=lib mocha --recursive --reporter spec --compilers js:babel-core/register --require babel-polyfill --watch",
    "eslint": "eslint ./src --fix",
    "prepublish": "npm run build",
    "docs": "jsdoc2md -c .jsdoc ./src/**/*.js --template ./src/README.hbs > README.md"
  }
}
